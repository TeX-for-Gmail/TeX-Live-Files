%% M %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
xucchar "M";
beginchar("M", mxzw#+2mxone#, mxheight#, 0);
   pickup xuc_pen;
   lft z8 = (mxone, mxheight-.5mxtop);
   z1 = (x8, mxlittle);
   lft bot z2 = (mxhalf, 0);
   lft z3 = (0, .5mxlittle);
   z4 = (x2, mxlittle);
   z5 = (x8+.5mxone, y4);
   z6 = (x8+mxone, y3);
   z7 = (x6, y2);
   draw z8--z1{down}..{left}z2..z3..{right}z4--z5{right}..{down}z6--z7;
   xuc_haken_gross(8);
   xuc_serif(7, 1);
   xuc_halbserif(8, -1, -1);
   penlabels(range 1 thru 8);
endchar;


