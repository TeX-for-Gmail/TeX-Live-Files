beginchar("Z", mxzw#+2mxone#, mxheight#, 0);
   pickup xuc_pen;
   lft bot z1 = (mxone, .5mxlittle);
   z2 = (x1, mxheight-.5mxtop);
   draw z1--z2;

   z5 = (x1, mxlittle);
   z7 = (x1+mxone, .5mxlittle);
   z6 = (.5[x5, x7], y5);
   bot z8 = (.x6, 0);

   draw z5--z6{right}..z7..z8{left}..{up}z1;

   xuc_halbserif(2, -1, -1);
%   xuc_haken(2);
   xuc_haken_gross(2);
   penlabels(range 1 thru 8);
endchar;


