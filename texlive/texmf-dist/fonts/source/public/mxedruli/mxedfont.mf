   z4 = (mxone, y2);
   z5 = (mxone-mxfeet, 0);
   draw z1..{up}z2..{right}z3..{down}z4..z5;
 
   z6 = (0, mxheight-.05mxtop);
   z9 = (mxhalf, mxheight);
   z7 = (x4-.5mxhalf,mxheight-.25mxtop);
   z10 = (x9, mxheight-.5mxtop);
   z8 = (0, mxheight-.45mxtop);
