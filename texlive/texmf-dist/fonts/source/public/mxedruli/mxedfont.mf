%% b %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% New Design
mxedchar "b - ban";
beginchar("b", mxzw#+mxone#, mxheight#, 0);
  if known cmbase: adjust_fit(0,0); fi
  pickup mxed_pen;
  z1 = (0, .5mxlittle);
  z2 = (mxhalf, 0);
  z3 = (mxone, y1);
  z4 = (x2, mxlittle);
  draw z1{down}..{right}z2..{up}z3..{left}z4..cycle;
 
  
%   top z5 = (x1, mxheight); 
%   z6 = (x5, mxheight);
%   z7 = (x4, y6-.3mxtop);
%   draw z5--z6--z7--z4;
  pickup mxed_finepen;
   top z5 = (x1, mxheight); 
   z6 = (x5, mxheight);
   z7 = (x4, y6-.5mxtop);
   draw z4..{up}z7..z5..{up}z6;

  penlabels(range 1 thru 7);
endchar;

 
