   z31 = (x3, .33mxlittle);
   lft z5 = (0, .7mxlittle);
   draw z1{down}..{right}z2..{up}z31..{up}z3..{left}z4..z5;
 
   z6 = (x3, 1.5mxlittle);
   z7 = (x4-.25mxthird, 1.75mxlittle);
   z8 =(x6, mxheight);
   draw z4{right}..{up}z6..{dir 90}z7..z8;
 
   penlabels(range 1 thru 8);
endchar;
 


%% long ee %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
